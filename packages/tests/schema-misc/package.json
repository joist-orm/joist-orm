    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "jest": "^29.3.1",
    "jest-junit": "^14.0.1",
    "joist-codegen": "workspace:*",
    "joist-migration-utils": "workspace:*",
    "joist-test-utils": "workspace:*",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "superstruct": "^0.15.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.9.3"
